               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Ports-_packagedElement.10-conjugatedPortTyping" name="conjugatedPortTyping" visibility="public" isDerived="true" type="Systems-Ports-ConjugatedPortTyping" association="Systems-Ports-_packagedElement.10">
                  <subsettedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Elements-_packagedElement.2-targetRelationship"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-_packagedElement.10-conjugatedPortTyping-_ownedComment.0" body="&lt;p&gt;The ConjugatedPortTypings whose &lt;code&gt;portDefinition&lt;/code&gt; is a certain PortDefinition.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-_packagedElement.10-conjugatedPortTyping"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-_packagedElement.10-conjugatedPortTyping-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Ports-_packagedElement.10-conjugatedPortTyping-_upperValue" value="*"/>
