            <packagedElement xmi:type="uml:Class" xmi:id="Systems-DefinitionAndUsage-VariantMembership" name="VariantMembership">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-VariantMembership-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;VariantMembership&lt;/code&gt; is a &lt;code&gt;Membership&lt;/code&gt; between a variation point &lt;code&gt;Definition&lt;/code&gt; or &lt;code&gt;Usage&lt;/code&gt; and a &lt;code&gt;Usage&lt;/code&gt; that represents a variant in the context of that variation. The &lt;code&gt;membershipOwningNamespace&lt;/code&gt; for the &lt;code&gt;VariantMembership&lt;/code&gt; must be either a Definition or a &lt;code&gt;Usage&lt;/code&gt; with &lt;code&gt;isVariation = true&lt;/code&gt;.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-DefinitionAndUsage-VariantMembership"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace" name="validateVariantMembershipOwningNamespace">
                  <constrainedElement xmi:idref="Systems-DefinitionAndUsage-VariantMembership"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;membershipOwningNamespace&lt;/code&gt; of a &lt;code&gt;VariantMembership&lt;/code&gt; must be a variation-point &lt;code&gt;Definition&lt;/code&gt; or &lt;code&gt;Usage&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace-_specification">
                     <body>membershipOwningNamespace.oclIsKindOf(Definition) and
    membershipOwningNamespace.oclAsType(Definition).isVariation or
membershipOwningNamespace.oclIsKindOf(Usage) and
    membershipOwningNamespace.oclAsType(Usage).isVariation
</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-DefinitionAndUsage-VariantMembership-_generalization.0">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Namespaces-OwningMembership"/>
