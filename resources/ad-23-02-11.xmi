            <packagedElement xmi:type="uml:Class" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage" name="ReferenceUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ReferenceUsage&lt;/code&gt; is a &lt;code&gt;Usage&lt;/code&gt; that specifies a non-compositional (&lt;code&gt;isComposite = false&lt;/code&gt;) reference to something. The &lt;code&gt;definition&lt;/code&gt; of a &lt;code&gt;ReferenceUsage&lt;/code&gt; can be any kind of &lt;code&gt;Classifier&lt;/code&gt;, with the default being the top-level &lt;code&gt;Classifier&lt;/code&gt; &lt;code&gt;&lt;em&gt;Base::Anything&lt;/em&gt;&lt;/code&gt; from the Kernel Semantic Library. This allows the specification of a generic reference without distinguishing if the thing referenced is an attribute value, item, action, etc.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-validateReferenceUsageIsReference" name="validateReferenceUsageIsReference">
                  <constrainedElement xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-validateReferenceUsageIsReference-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ReferenceUsage&lt;/code&gt; is always referential.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage-validateReferenceUsageIsReference"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-validateReferenceUsageIsReference-_specification">
                     <body>isReference</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-_generalization.0" general="Systems-DefinitionAndUsage-Usage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-isReference" name="isReference" visibility="public" isDerived="true">
                  <type href="http://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#Boolean"/>
                  <redefinedProperty xmi:idref="Systems-DefinitionAndUsage-Usage-isReference"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-isReference-_ownedComment.0" body="&lt;p&gt;Always &lt;code&gt;true&lt;/code&gt; for a &lt;code&gt;ReferenceUsage&lt;/code&gt;.&lt;/code&gt;">
                     <annotatedElement xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage-isReference"/>
                  </ownedComment>
                  <defaultValue xmi:type="uml:LiteralString" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-isReference-_defaultValue" value="true"/>
               </ownedAttribute>
               <ownedOperation xmi:type="uml:Operation" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature" name="namingFeature" visibility="public" bodyCondition="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-unnamed1">
                  <redefinedOperation xmi:idref="Systems-DefinitionAndUsage-Usage-namingFeature"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-_ownedComment.0" body="&lt;p&gt;If this &lt;code&gt;ReferenceUsage&lt;/code&gt; is the &lt;em&gt;&lt;code&gt;payload&lt;/code&gt;&lt;/em&gt; &lt;code&gt;parameter&lt;/code&gt; of a &lt;code&gt;TransitionUsage&lt;/code&gt;, then its naming &lt;code&gt;Feature&lt;/code&gt; is the &lt;code&gt;payloadParameter&lt;/code&gt; of the &lt;code&gt;triggerAction&lt;/code&gt; of that &lt;code&gt;TransitionUsage&lt;/code&gt; (if any).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature"/>
                  </ownedComment>
                  <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-unnamed1" name="unnamed1">
                     <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-unnamed1-_specification">
                        <body>if owningType &lt;&gt; null and owningType.oclIsKindOf(TransitionUsage) and
    owningType.oclAsType(TransitionUsage).inputParameter(2) = self then
    owningType.oclAsType(TransitionUsage).triggerPayloadParameter()
else self.oclAsType(Usage).namingFeature()
endif</body>
                        <language>OCL2.0</language>
                     </specification>
                  </ownedRule>
                  <ownedParameter xmi:type="uml:Parameter" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-_ownedParameter.0" visibility="public" direction="return" effect="create">
                     <type href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Features-Feature"/>
                     <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-DefinitionAndUsage-ReferenceUsage-namingFeature-_ownedParameter.0-_lowerValue"/>
                  </ownedParameter>
               </ownedOperation>
            </packagedElement>
