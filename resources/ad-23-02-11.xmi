                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Parts-PartUsage-partDefinition-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Parts-PartUsage-partDefinition-_upperValue" value="*"/>
               </ownedAttribute>
