         </packagedElement>
         <packagedElement xmi:type="uml:Package" xmi:id="Systems-Occurrences" name="Occurrences">
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Occurrences-LifeClass" name="LifeClass">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-_ownedComment.0" body="&lt;p&gt;A &lt;coed&gt;LifeClass&lt;/code&gt; is a &lt;code&gt;Class&lt;/code&gt; that specializes both the &lt;code&gt;Class&lt;/code&gt; &lt;code&gt;&lt;em&gt;Occurrences::Life&lt;/em&gt;&lt;/code&gt; from the Kernel Semantic Library and a single &lt;code&gt;OccurrenceDefinition&lt;/code&gt;, and has a multiplicity of 0..1. This constrains the &lt;code&gt;OccurrenceDefinition&lt;/code&gt; being specialized to have at most one instance that is a complete &lt;code&gt;Life&lt;/code&gt;.&lt;/p&gt;&#xA;">
                  <annotatedElement xmi:idref="Systems-Occurrences-LifeClass"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassLifeSpecialization" name="checkLifeClassLifeSpecialization">
                  <constrainedElement xmi:idref="Systems-Occurrences-LifeClass"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassLifeSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;LifeClass&lt;/code&gt; must specialize the &lt;code&gt;Class&lt;/code&gt; &lt;code&gt;&lt;em&gt;Occurrences::Life&lt;/em&gt;&lt;/code&gt; from the Kernel Semantic Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-checkLifeClassLifeSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassLifeSpecialization-_specification">
                     <body>specializesFromLibrary("Occurrences::Life")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassMultiplicitySpecialization" name="checkLifeClassMultiplicitySpecialization">
                  <constrainedElement xmi:idref="Systems-Occurrences-LifeClass"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassMultiplicitySpecialization-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;multiplicity&lt;/code&gt; of a &lt;code&gt;LifeClass&lt;/code&gt; is &lt;em&gt;&lt;code&gt;zeroOrOne&lt;/code&gt;&lt;/em&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-checkLifeClassMultiplicitySpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassMultiplicitySpecialization-_specification">
                     <body>multiplicity &lt;&gt; null and
multiplicity.specializesFromLibrary("Base::zeroOrOne")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassOccurrenceSpecialization" name="checkLifeClassOccurrenceSpecialization">
                  <constrainedElement xmi:idref="Systems-Occurrences-LifeClass"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassOccurrenceSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;LifeClass&lt;/code&gt; must specialize its &lt;code&gt;individualDefinition&lt;/code&gt;.&lt;/code&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-checkLifeClassOccurrenceSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-LifeClass-checkLifeClassOccurrenceSpecialization-_specification">
                     <body>specializes(individualDefinition)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-LifeClass-validateLifeClassIsSufficient" name="validateLifeClassIsSufficient">
                  <constrainedElement xmi:idref="Systems-Occurrences-LifeClass"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-validateLifeClassIsSufficient-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;LifeClass&lt;/code&gt; always has &lt;code&gt;isSufficient = true&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-validateLifeClassIsSufficient"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-LifeClass-validateLifeClassIsSufficient-_specification">
                     <body>isSufficient</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Occurrences-LifeClass-_generalization.0">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Classes-Class"/>
               </generalization>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Occurrences-LifeClass-isSufficient" name="isSufficient" visibility="public">
                  <type href="http://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#Boolean"/>
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-Type-isSufficient"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-isSufficient-_ownedComment.0" body="&lt;p&gt;Always true for a &lt;code&gt;LifeClass&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-isSufficient"/>
                  </ownedComment>
                  <defaultValue xmi:type="uml:LiteralBoolean" xmi:id="Systems-Occurrences-LifeClass-isSufficient-_defaultValue" value="true"/>
               </ownedAttribute>
               <ownedOperation xmi:type="uml:Operation" xmi:id="Systems-Occurrences-LifeClass-effectiveName" name="effectiveName" visibility="public" bodyCondition="Systems-Occurrences-LifeClass-effectiveName-unnamed1">
                  <redefinedOperation href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Elements-Element-effectiveName"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-LifeClass-effectiveName-_ownedComment.0" body="&lt;p&gt;The name of a &lt;code&gt;LifeClass&lt;/code&gt; is &lt;em&gt;&lt;code&gt;Life&lt;/code&gt;&lt;em&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-LifeClass-effectiveName"/>
                  </ownedComment>
                  <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-LifeClass-effectiveName-unnamed1" name="unnamed1">
                     <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-LifeClass-effectiveName-unnamed1-_specification">
                        <body>"Life"</body>
                        <language>OCL2.0</language>
                     </specification>
                  </ownedRule>
                  <ownedParameter xmi:type="uml:Parameter" xmi:id="Systems-Occurrences-LifeClass-effectiveName-_ownedParameter.0" visibility="public" direction="return" effect="create">
                     <type href="http://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#String"/>
                     <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Occurrences-LifeClass-effectiveName-_ownedParameter.0-_lowerValue"/>
                  </ownedParameter>
               </ownedOperation>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Occurrences-_packagedElement.1" visibility="private">
               <memberEnd xmi:idref="Systems-Occurrences-_packagedElement.1-individualUsage"/>
               <memberEnd xmi:idref="Systems-Occurrences-OccurrenceUsage-individualDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Occurrences-_packagedElement.1-individualUsage" name="individualUsage" visibility="public" isDerived="true" type="Systems-Occurrences-OccurrenceUsage" association="Systems-Occurrences-_packagedElement.1">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-_packagedElement.1-individualUsage-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;OccurrenceUsage&lt;/code&gt; that has a certain &lt;code&gt;individualDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-_packagedElement.1-individualUsage"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Occurrences-_packagedElement.1-individualUsage-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Occurrences-_packagedElement.1-individualUsage-_upperValue" value="*"/>
               </ownedEnd>
            </packagedElement>
            <packagedElement xmi:type="uml:Enumeration" xmi:id="Systems-Occurrences-PortionKind" name="PortionKind">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-PortionKind-_ownedComment.0" body="&lt;p&gt;&lt;code&gt;PortionKind&lt;/code&gt; is an enumeration of the specific kinds of &lt;code&gt;&lt;em&gt;Occurrence&lt;/em&gt;&lt;/code&gt; portions that can be represented by an &lt;code&gt;OccurrenceUsage&lt;/code&gt;.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Occurrences-PortionKind"/>
               </ownedComment>
               <ownedLiteral xmi:type="uml:EnumerationLiteral" xmi:id="Systems-Occurrences-PortionKind-timeslice" name="timeslice">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-PortionKind-timeslice-_ownedComment.0" body="&lt;p&gt;A time slice of an &lt;code&gt;Occurrence&lt;/code&gt; (a portion over time).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-PortionKind-timeslice"/>
                  </ownedComment>
               </ownedLiteral>
               <ownedLiteral xmi:type="uml:EnumerationLiteral" xmi:id="Systems-Occurrences-PortionKind-snapshot" name="snapshot">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-PortionKind-snapshot-_ownedComment.0" body="&lt;p&gt;A snapshot of an &lt;code&gt;Occurrence&lt;/code&gt; (a time slice with zero duration).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-PortionKind-snapshot"/>
                  </ownedComment>
               </ownedLiteral>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Occurrences-OccurrenceUsage" name="OccurrenceUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;OccurrenceUsage&lt;/code&gt; is a &lt;code&gt;Usage&lt;/code&gt; whose &lt;code&gt;types&lt;/code&gt; are all &lt;code&gt;Classes&lt;/code&gt;. Nominally, if a &lt;code&gt;type&lt;/code&gt; is an &lt;code&gt;OccurrenceDefinition&lt;/code&gt;, an &lt;code&gt;OccurrenceUsage&lt;/code&gt; is a &lt;code&gt;Usage&lt;/code&gt; of that &lt;code&gt;OccurrenceDefinition&lt;/code&gt; within a system. However, other types of Kernel &lt;code&gt;Classes&lt;/code&gt; are also allowed, to permit use of &lt;code&gt;Classes&lt;/code&gt; from the Kernel Model Libraries.&lt;/p&gt;&#xA;">
                  <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-deriveOccurrenceUsageIndividualDefinition" name="deriveOccurrenceUsageIndividualDefinition">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-deriveOccurrenceUsageIndividualDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;individualDefinition&lt;/code&gt; of an &lt;code&gt;OccurrenceUsage&lt;/code&gt; is the &lt;code&gt;occurrenceDefinition&lt;/code&gt; that is an &lt;code&gt;OccurrenceDefinition&lt;/code&gt; with &lt;code&gt;isIndividual = true&lt;/code&gt;, if any.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-deriveOccurrenceUsageIndividualDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-deriveOccurrenceUsageIndividualDefinition-_specification">
                     <body>individualDefinition =
    let individualDefinitions : OrderedSet(OccurrenceDefinition) = 
        occurrenceDefinition-&gt;
            selectByKind(OccurrenceDefinition)-&gt;
            select(isIndividual) in
    if individualDefinitions-&gt;isEmpty() then null
    else individualDefinitions-&gt;first() endif</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualUsage" name="validateOccurrenceUsageIndividualUsage">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualUsage-_ownedComment.0" body="&lt;p&gt;If an &lt;code&gt;OccurrenceUsage&lt;/code&gt; has &lt;code&gt;isIndividual = true&lt;/code&gt;, then it must have an &lt;code&gt;individualDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualUsage"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualUsage-_specification">
                     <body>isIndividual implies individualDefinition &lt;&gt; null</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSpecialization" name="checkOccurrenceUsageSpecialization">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSpecialization-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;OccurrenceUsage&lt;/code&gt; must directly or indirectly specialize &lt;code&gt;&lt;em&gt;Occurrences::occurrences&lt;/em&gt;&lt;/code&gt; from the Kernel Semantic Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSpecialization-_specification">
                     <body>specializesFromLibrary("Occurrences::occurrences")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSuboccurrenceSpecialization" name="checkOccurrenceUsageSuboccurrenceSpecialization">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSuboccurrenceSpecialization-_ownedComment.0" body="&lt;p&gt;A composite &lt;code&gt;OccurrenceUsage&lt;code&gt;, whose &lt;code&gt;ownedType&lt;/code&gt; is a &lt;code&gt;Class&lt;/code&gt;, another &lt;code&gt;OccurrenceUsage&lt;/code&gt;, or any kind of &lt;code&gt;Feature&lt;/code&gt; typed by a &lt;code&gt;Class&lt;code&gt;, must directly or indirectly specialize &lt;em&gt;&lt;code&gt;Occurrences::Occurrence::suboccurrences&lt;/code&gt;&lt;/em&gt;.">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSuboccurrenceSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageSuboccurrenceSpecialization-_specification">
                     <body>isComposite and
owningType &lt;&gt; null and
(owningType.oclIsKindOf(Class) or
 owningType.oclIsKindOf(OccurrenceUsage) or
 owningType.oclIsKindOf(Feature) and
    owningType.oclAsType(Feature).type-&gt;
        exists(oclIsKind(Class))) implies
    specializesFromLibrary("Occurrences::Occurrence::suboccurrences")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualDefinition" name="validateOccurrenceUsageIndividualDefinition">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualDefinition-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;OccurrenceUsage&lt;/code&gt; must have at most one &lt;code&gt;occurrenceDefinition&lt;/code&gt; with &lt;code&gt;isIndividual = true&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-validateOccurrenceUsageIndividualDefinition-_specification">
                     <body>occurrenceDefinition-&gt;select(isIndividual).size() &lt;= 1</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageTypeFeaturing" name="checkOccurrenceUsageTypeFeaturing">
                  <constrainedElement xmi:idref="Systems-Occurrences-OccurrenceUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageTypeFeaturing-_ownedComment.0" body="&lt;p&gt;If the &lt;code&gt;portionKind&lt;/code&gt; of an &lt;code&gt;OccurrenceUsage&lt;/code&gt; is not empty, then, for each &lt;code&gt;occurrenceDefinition&lt;/code&gt; of the &lt;code&gt;OccurrenceUsage&lt;/code&gt;, there must be a &lt;code&gt;featuringType&lt;/code&gt; of the &lt;code&gt;OccurrenceUsage&lt;/code&gt; which either is the &lt;code&gt;occurrenceDefinition&lt;/code&gt; or directly or indirectly specializes it.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageTypeFeaturing"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Occurrences-OccurrenceUsage-checkOccurrenceUsageTypeFeaturing-_specification">
                     <body>portionKind &lt;&gt; null implies
    occurrenceDefinition-&gt;forAll(occ | 
        featuringType-&gt;exists(specializes(occ)))</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Occurrences-OccurrenceUsage-_generalization.0" general="Systems-DefinitionAndUsage-Usage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition" name="occurrenceDefinition" visibility="public" isOrdered="true" isDerived="true" association="Systems-Occurrences-_packagedElement.7">
                  <type href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Classes-Class"/>
                  <redefinedProperty xmi:idref="Systems-DefinitionAndUsage-Usage-definition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;Classes&lt;/code&gt; that are the types of this &lt;code&gt;OccurrenceUsage&lt;/code&gt;. Nominally, these are &lt;code&gt;OccurrenceDefinitions&lt;/code&gt;, but other kinds of kernel &lt;code&gt;Classes&lt;/code&gt; are also allowed, to permit use of &lt;code&gt;Classes&lt;/code&gt; from the Kernel Model Libraries.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition-_upperValue" value="*"/>
               </ownedAttribute>
