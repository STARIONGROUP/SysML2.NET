                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-DefinitionAndUsage-Usage-deriveUsageVariantMembership-_specification">
                     <body>variantMembership = ownedMembership-&gt;selectByKind(VariantMembership)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-DefinitionAndUsage-Usage-validateUsageNonVariationMembership" name="validateUsageNonVariationMembership">
                  <constrainedElement xmi:idref="Systems-DefinitionAndUsage-Usage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-DefinitionAndUsage-Usage-validateUsageNonVariationMembership-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;Usage&lt;/code&gt; that is not a variation must not have any &lt;code&gt;variantMemberships&lt;/code&gt;.&lt;/p&gt;">
