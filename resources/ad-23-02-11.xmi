               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Views-_packagedElement.19-owningViewDefinition" name="owningViewDefinition" visibility="public" isDerived="true" type="Systems-Views-ViewDefinition" association="Systems-Views-_packagedElement.19">
