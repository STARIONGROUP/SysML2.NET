               <memberEnd xmi:idref="Systems-Interfaces-InterfaceUsage-interfaceDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Interfaces-_packagedElement.2-definedInterface" name="definedInterface" visibility="public" isDerived="true" type="Systems-Interfaces-InterfaceUsage" association="Systems-Interfaces-_packagedElement.2">
