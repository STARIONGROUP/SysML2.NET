                     <annotatedElement xmi:idref="Systems-Views-ViewRenderingMembership-validateViewRenderingMembershipOwningType"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewRenderingMembership-validateViewRenderingMembershipOwningType-_specification">
                     <body>owningType.oclIsKindOf(ViewDefinition) or
owningType.oclIsKindOf(ViewUsage)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
