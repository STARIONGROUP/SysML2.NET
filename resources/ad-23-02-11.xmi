                  <annotatedElement xmi:idref="Systems-Views-ViewRenderingMembership"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewRenderingMembership-deriveVewRenderingMembershipReferencedRendering" name="deriveVewRenderingMembershipReferencedRendering">
                  <constrainedElement xmi:idref="Systems-Views-ViewRenderingMembership"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewRenderingMembership-deriveVewRenderingMembershipReferencedRendering-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;referencedRendering&lt;/code&gt; of a &lt;code&gt;ViewRenderingMembership&lt;/code&gt; is the &lt;code&gt;referencedFeature&lt;/code&gt; of the &lt;code&gt;ownedReferenceSubsetting&lt;/code&gt; (which must be a &lt;code&gt;RenderingUsage&lt;/code&gt;) of the &lt;code&gt;ownedRendering&lt;/code&gt;, if there is one, and, otherwise, the &lt;code&gt;ownedRendering&lt;/code&gt; itself.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-ViewRenderingMembership-deriveVewRenderingMembershipReferencedRendering"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewRenderingMembership-deriveVewRenderingMembershipReferencedRendering-_specification">
                     <body>referencedRendering =
    let reference: ReferenceSubsetting = 
        ownedRendering.ownedReferenceSubsetting in
    if reference = null then ownedRendering
    else if not reference.referencedFeature.oclIsKindOf(RenderingUsage) then null
    else reference.referencedFeature.oclAsType(RenderingUsage)
    endif</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewRenderingMembership-validateViewRenderingMembershipOwningType" name="validateViewRenderingMembershipOwningType">
