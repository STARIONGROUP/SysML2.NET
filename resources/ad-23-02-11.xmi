                     <annotatedElement xmi:idref="Systems-Cases-CaseUsage-checkCaseUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseUsage-checkCaseUsageSpecialization-_specification">
                     <body>specializeFromLibrary('Cases::cases')</body>
