            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Allocations-_packagedElement.3" visibility="private">
               <memberEnd xmi:idref="Systems-Allocations-_packagedElement.3-featuringAllocationDefinition"/>
               <memberEnd xmi:idref="Systems-Allocations-AllocationDefinition-allocation"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Allocations-_packagedElement.3-featuringAllocationDefinition" name="featuringAllocationDefinition" visibility="public" isDerived="true" type="Systems-Allocations-AllocationDefinition" association="Systems-Allocations-_packagedElement.3">
                  <subsettedProperty xmi:idref="Systems-DefinitionAndUsage-_packagedElement.1-featuringDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Allocations-_packagedElement.3-featuringAllocationDefinition-_ownedComment.0" body="&lt;p&gt;The AllocationDefinitions that feature a certain &lt;code&gt;allocation&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Allocations-_packagedElement.3-featuringAllocationDefinition"/>
                  </ownedComment>
