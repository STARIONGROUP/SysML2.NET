                  <redefinedProperty xmi:idref="Systems-Connections-ConnectionDefinition-connectionEnd"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Interfaces-InterfaceDefinition-interfaceEnd-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;PortUsages&lt;/code&gt; that are the &lt;code&gt;connectionEnds&lt;/code&gt; of this &lt;code&gt;InterfaceDefinition&lt;/code&gt;.&#xA;&#xA;">
                     <annotatedElement xmi:idref="Systems-Interfaces-InterfaceDefinition-interfaceEnd"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Interfaces-InterfaceDefinition-interfaceEnd-_lowerValue"/>
