                     <annotatedElement xmi:idref="Systems-DefinitionAndUsage-Definition-deriveDefinitionVariant"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-DefinitionAndUsage-Definition-deriveDefinitionVariant-_specification">
                     <body>variant = variantMembership.ownedVariantUsage</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
