         <packagedElement xmi:type="uml:Package" xmi:id="Systems-Items" name="Items">
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Items-ItemUsage" name="ItemUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemUsage-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;ItemUsage&lt;/code&gt; is a &lt;code&gt;ItemUsage&lt;/code&gt; whose &lt;code&gt;definition&lt;/code&gt; is a &lt;code&gt;Structure&lt;/code&gt;. Nominally, if the &lt;code&gt;definition&lt;/code&gt; is an &lt;code&gt;ItemDefinition&lt;/code&gt;, an &lt;code&gt;ItemUsage&lt;/code&gt; is a &lt;code&gt;ItemUsage&lt;/code&gt; of that &lt;code&gt;ItemDefinition&lt;/code&gt; within a system. However, other kinds of Kernel &lt;code&gt;Structures&lt;/code&gt; are also allowed, to permit use of &lt;code&gt;Structures&lt;/code&gt; from the Kernel Model Libraries.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Items-ItemUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Items-ItemUsage-deriveItemUsageItemDefinition" name="deriveItemUsageItemDefinition">
                  <constrainedElement xmi:idref="Systems-Items-ItemUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemUsage-deriveItemUsageItemDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;itemDefinitions&lt;/code&gt; of an &lt;code&gt;ItemUsage&lt;/code&gt; are those &lt;code&gt;occurrenceDefinitions&lt;/code&gt; that are &lt;code&gt;ItemDefinitions&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Items-ItemUsage-deriveItemUsageItemDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Items-ItemUsage-deriveItemUsageItemDefinition-_specification">
                     <body>itemDefinition = occurrenceDefinition-&gt;selectByKind(ItemDefinition)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Items-ItemUsage-checkItemUsageSpecialization" name="checkItemUsageSpecialization">
                  <constrainedElement xmi:idref="Systems-Items-ItemUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemUsage-checkItemUsageSpecialization-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;ItemUsage&lt;/code&gt; must directly or indirectly specialize the Systems Model Library &lt;code&gt;ItemUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;items&lt;/code&gt;&lt;/em&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Items-ItemUsage-checkItemUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Items-ItemUsage-checkItemUsageSpecialization-_specification">
                     <body>specializesFromLibrary("Items::items")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Items-ItemUsage-checkItemUsageSubitemSpecialization" name="checkItemUsageSubitemSpecialization">
                  <constrainedElement xmi:idref="Systems-Items-ItemUsage"/>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Items-ItemUsage-checkItemUsageSubitemSpecialization-_specification">
                     <body>isComposite and owningType &lt;&gt; null and
(owningType.oclIsKindOf(ItemDefinition) or
 owningType.oclIsKindOf(ItemUsage)) implies
    specializesFromLibrary("Items::Item::subitem")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Items-ItemUsage-_generalization.0" general="Systems-Occurrences-OccurrenceUsage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Items-ItemUsage-itemDefinition" name="itemDefinition" visibility="public" isOrdered="true" isDerived="true" association="Systems-DefinitionAndUsage-_packagedElement.4">
                  <type href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Structures-Structure"/>
                  <subsettedProperty xmi:idref="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemUsage-itemDefinition-_ownedComment.0" body="&lt;p&gt;The Structures that are the &lt;code&gt;definitions&lt;/code&gt; of this ItemUsage. Nominally, these are ItemDefinitions, but other kinds of Kernel Structures are also allowed, to permit use of Structures from the Kernel Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Items-ItemUsage-itemDefinition"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Items-ItemUsage-itemDefinition-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Items-ItemUsage-itemDefinition-_upperValue" value="*"/>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Items-ItemDefinition" name="ItemDefinition">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemDefinition-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;ItemDefinition&lt;/code&gt; is an &lt;code&gt;OccurrenceDefinition&lt;/code&gt; of the &lt;code&gt;Structure&lt;/code&gt; of things that may themselves be systems or parts of systems, but may also be things that are acted on by a system or parts of a system, but which do not necessarily perform actions themselves. This includes items that can be exchanged between parts of a system, such as water or electrical signals.&lt;/p&gt;&#xA;">
                  <annotatedElement xmi:idref="Systems-Items-ItemDefinition"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Items-ItemDefinition-checkItemDefinitionSpecialization" name="checkItemDefinitionSpecialization">
                  <constrainedElement xmi:idref="Systems-Items-ItemDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Items-ItemDefinition-checkItemDefinitionSpecialization-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;ItemDefinition&lt;/code&gt; must directly or indirectly specialize the Systems Library Model &lt;code&gt;ItemDefinition&lt;/code&gt; &lt;em&gt;&lt;code&gt;Items::Item&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Items-ItemDefinition-checkItemDefinitionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Items-ItemDefinition-checkItemDefinitionSpecialization-_specification">
                     <body>specializesFromLibrary("Items::Item")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Items-ItemDefinition-_generalization.0">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Structures-Structure"/>
               </generalization>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Items-ItemDefinition-_generalization.1" general="Systems-Occurrences-OccurrenceDefinition"/>
            </packagedElement>
         </packagedElement>
         <packagedElement xmi:type="uml:Package" xmi:id="Systems-Views" name="Views">
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Views-ViewpointDefinition" name="ViewpointDefinition">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewpointDefinition-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ViewpointDefinition&lt;/code&gt; is a &lt;code&gt;RequirementDefinition&lt;/code&gt; that specifies one or more stakeholder concerns that are to be satisfied by creating a view of a model.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Views-ViewpointDefinition"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewpointDefinition-deriveViewpointDefinitionViewpointStakeholder" name="deriveViewpointDefinitionViewpointStakeholder">
                  <constrainedElement xmi:idref="Systems-Views-ViewpointDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewpointDefinition-deriveViewpointDefinitionViewpointStakeholder-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;viewpointStakeholders&lt;/code&gt; of a &lt;code&gt;ViewpointDefinition&lt;/code&gt; are the &lt;code&gt;ownedStakeholderParameters&lt;/code&gt; of all &lt;code&gt;featureMemberships&lt;/code&gt; that are &lt;code&gt;StakeholderMemberships&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-ViewpointDefinition-deriveViewpointDefinitionViewpointStakeholder"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewpointDefinition-deriveViewpointDefinitionViewpointStakeholder-_specification">
                     <body>viewpointStakeholder = framedConcern.featureMemberhsip-&gt;
    selectByKind(StakeholderMembership).
    ownedStakeholderParameter</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewpointDefinition-checkViewpointDefinitionSpecialization" name="checkViewpointDefinitionSpecialization">
                  <constrainedElement xmi:idref="Systems-Views-ViewpointDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewpointDefinition-checkViewpointDefinitionSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ViewpointDefinition&lt;/code&gt; must directly or indirectly specialize the base &lt;code&gt;ViewpointDefinition&lt;/code&gt; &lt;code&gt;&lt;em&gt;Views::Viewpoint&lt;/em&gt;&lt;/code&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-ViewpointDefinition-checkViewpointDefinitionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewpointDefinition-checkViewpointDefinitionSpecialization-_specification">
                     <body>specializesFromLibrary('Views::Viewpoint')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Views-ViewpointDefinition-_generalization.0" general="Systems-Requirements-RequirementDefinition"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Views-ViewpointDefinition-viewpointStakeholder" name="viewpointStakeholder" visibility="public" isOrdered="true" isDerived="true" type="Systems-Parts-PartUsage" association="Systems-Views-_packagedElement.20">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewpointDefinition-viewpointStakeholder-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;PartUsages&lt;/code&gt; that identify the stakeholders with concerns framed by this &lt;code&gt;ViewpointDefinition&lt;/code&gt;, which are the owned and inherited &lt;code&gt;stakeholderParameters&lt;/code&gt; of the &lt;code&gt;framedConcerns&lt;/code&gt; of this &lt;code&gt;ViewpointDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-ViewpointDefinition-viewpointStakeholder"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Views-ViewpointDefinition-viewpointStakeholder-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Views-ViewpointDefinition-viewpointStakeholder-_upperValue" value="*"/>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Views-_packagedElement.1" visibility="private">
               <memberEnd xmi:idref="Systems-Views-_packagedElement.1-exposingView"/>
               <memberEnd xmi:idref="Systems-Views-ViewUsage-exposedElement"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Views-_packagedElement.1-exposingView" name="exposingView" visibility="public" isDerived="true" type="Systems-Views-ViewUsage" association="Systems-Views-_packagedElement.1">
                  <subsettedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Namespaces-_packagedElement.5-namespace"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-_packagedElement.1-exposingView-_ownedComment.0" body="&lt;p&gt;A ViewUsage exposing a certain &lt;code&gt;exposedElement&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-_packagedElement.1-exposingView"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Views-_packagedElement.1-exposingView-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Views-_packagedElement.1-exposingView-_upperValue" value="*"/>
               </ownedEnd>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Views-_packagedElement.2" visibility="private">
               <memberEnd xmi:idref="Systems-Views-_packagedElement.2-definedRendering"/>
               <memberEnd xmi:idref="Systems-Views-RenderingUsage-renderingDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Views-_packagedElement.2-definedRendering" name="definedRendering" visibility="public" isDerived="true" type="Systems-Views-RenderingUsage" association="Systems-Views-_packagedElement.2">
                  <subsettedProperty xmi:idref="Systems-Parts-_packagedElement.2-definedPart"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-_packagedElement.2-definedRendering-_ownedComment.0" body="&lt;p&gt;The RenderingUsages defined by a certain &lt;code&gt;renderingDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-_packagedElement.2-definedRendering"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Views-_packagedElement.2-definedRendering-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Views-_packagedElement.2-definedRendering-_upperValue" value="*"/>
               </ownedEnd>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Views-ViewUsage" name="ViewUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewUsage-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ViewUsage&lt;/code&gt; is a usage of a &lt;code&gt;ViewDefinition&lt;/code&gt; to specify the generation of a view of the &lt;code&gt;members&lt;/code&gt; of a collection of &lt;code&gt;exposedNamespaces&lt;/code&gt;. The &lt;code&gt;ViewUsage&lt;/code&gt; can satisfy more &lt;code&gt;viewpoints&lt;/code&gt; than its definition, and it can specialize the &lt;code&gt;viewRendering&lt;/code&gt; specified by its definition.&lt;p&gt;">
                  <annotatedElement xmi:idref="Systems-Views-ViewUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewUsage-deriveViewUsageExposedElement" name="deriveViewUsageExposedElement">
                  <constrainedElement xmi:idref="Systems-Views-ViewUsage"/>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewUsage-deriveViewUsageExposedElement-_specification">
                     <body>exposedElement = ownedImport-&gt;selectByKind(Expose).
    importedMemberships(Set{}).memberElement-&gt;
    select(elm | includeAsExposed(elm))-&gt;
    asOrderedSet()</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewUsage-deriveViewUsageSatisfiedViewpoint" name="deriveViewUsageSatisfiedViewpoint">
                  <constrainedElement xmi:idref="Systems-Views-ViewUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewUsage-deriveViewUsageSatisfiedViewpoint-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;satisfiedViewpoints&lt;/code&gt; of a &lt;code&gt;ViewDefinition&lt;/code&gt; are its &lt;code&gt;ownedRequirements&lt;/code&gt; that are composite &lt;code&gt;ViewpointUsages&lt;/code&gt;. ">
                     <annotatedElement xmi:idref="Systems-Views-ViewUsage-deriveViewUsageSatisfiedViewpoint"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewUsage-deriveViewUsageSatisfiedViewpoint-_specification">
                     <body>satisfiedViewpoint = ownedRequirement-&gt;
    selectByKind(ViewpointUsage)-&gt;
    select(isComposite)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewUsage-deriveViewUsageViewCondition" name="deriveViewUsageViewCondition">
                  <constrainedElement xmi:idref="Systems-Views-ViewUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Views-ViewUsage-deriveViewUsageViewCondition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;viewConditions&lt;/code&gt; of a &lt;code&gt;ViewDefinition&lt;/code&gt; are the &lt;code&gt;conditions&lt;/code&gt; of its owned &lt;code&gt;ElementFilterMemberships&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Views-ViewUsage-deriveViewUsageViewCondition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Views-ViewUsage-deriveViewUsageViewCondition-_specification">
                     <body>viewCondition = featureMembership-&gt;
    selectByKind(ElementFilterMembership).
    condition</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Views-ViewUsage-deriveViewUsageViewRendering" name="deriveViewUsageViewRendering">
                  <constrainedElement xmi:idref="Systems-Views-ViewUsage"/>
