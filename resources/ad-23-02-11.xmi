               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Cases-CaseDefinition-objectiveRequirement" name="objectiveRequirement" visibility="public" isOrdered="true" isDerived="true" type="Systems-Requirements-RequirementUsage" association="Systems-Cases-_packagedElement.6">
