            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Cases-CaseDefinition" name="CaseDefinition">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;CaseDefinition&lt;/code&gt; is a &lt;code&gt;CalculationDefinition&lt;/code&gt; for a process, often involving collecting evidence or data, relative to a subject, possibly involving the collaboration of one or more other actors, producing a result that meets an objective.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Cases-CaseDefinition"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionObjectiveRequirement" name="deriveCaseDefinitionObjectiveRequirement">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionObjectiveRequirement-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;objectiveRequirement&lt;/code&gt; of a &lt;code&gt;CaseDefinition&lt;/code&gt; is the &lt;code&gt;ownedObjectiveRequirement&lt;/code&gt; of its &lt;case&gt;ObjectiveMembership&lt;/code&gt;, if any.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-deriveCaseDefinitionObjectiveRequirement"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionObjectiveRequirement-_specification">
                     <body>objectiveRequirement = 
    let objectives: OrderedSet(RequirementUsage) = 
        featureMembership-&gt;
            selectByKind(ObjectiveMembership).
            ownedRequirement in
    if objectives-&gt;isEmpty() then null
    else objectives-&gt;first().ownedObjectiveRequirement
    endif</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneObjective" name="validateCaseDefinitionOnlyOneObjective">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneObjective-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;CaseDefinition&lt;/code&gt; must have at most one &lt;code&gt;featureMembership&lt;/code&gt; that is a &lt;code&gt;ObjectiveMembership&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneObjective"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneObjective-_specification">
                     <body>featureMembership-&gt;
    selectByKind(ObjectiveMembership)-&gt;
    size() &lt;= 1</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionSubjectParameter" name="deriveCaseDefinitionSubjectParameter">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionSubjectParameter-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;subjectParameter&lt;/code&gt; of a &lt;code&gt;CaseDefinition&lt;/code&gt; is the &lt;code&gt;ownedSubjectParameter&lt;/code&gt; of its &lt;code&gt;SubjectMembership&lt;/code&gt; (if any).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-deriveCaseDefinitionSubjectParameter"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionSubjectParameter-_specification">
                     <body>subjectParameter =
    let subjectMems : OrderedSet(SubjectMembership) = 
        featureMembership-&gt;selectByKind(SubjectMembership) in
    if subjectMems-&gt;isEmpty() then null
    else subjectMems-&gt;first().ownedSubjectParameter
    endif</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionActorParameter" name="deriveCaseDefinitionActorParameter">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionActorParameter-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;actorParameters&lt;/code&gt; of a &lt;code&gt;CaseDefinition&lt;/code&gt; are the &lt;code&gt;ownedActorParameters&lt;/code&gt; of the &lt;code&gt;ActorMemberships&lt;/code&gt; of the &lt;code&gt;CaseDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-deriveCaseDefinitionActorParameter"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-deriveCaseDefinitionActorParameter-_specification">
                     <body>actorParameter = featureMembership-&gt;
    selectByKind(ActorMembership).
    ownedActorParameter</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneSubject" name="validateCaseDefinitionOnlyOneSubject">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneSubject-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;CaseDefinition&lt;/code&gt; must have at most one &lt;code&gt;featureMembership&lt;/code&gt; that is a &lt;code&gt;SubjectMembership&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneSubject"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionOnlyOneSubject-_specification">
                     <body>featureMembership-&gt;selectByKind(SubjectMembership)-&gt;size() &lt;= 1</body>
                     <language>English</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionSubjectParameterPosition" name="validateCaseDefinitionSubjectParameterPosition">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionSubjectParameterPosition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;subjectParameter&lt;/code&gt; of a &lt;code&gt;CaaseDefinition&lt;/code&gt; must be its first &lt;code&gt;input&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-validateCaseDefinitionSubjectParameterPosition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-validateCaseDefinitionSubjectParameterPosition-_specification">
                     <body>input-&gt;notEmpty() and input-&gt;first() = subjectParameter</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Cases-CaseDefinition-checkCaseDefinitionSpecialization" name="checkCaseDefinitionSpecialization">
                  <constrainedElement xmi:idref="Systems-Cases-CaseDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Cases-CaseDefinition-checkCaseDefinitionSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;CaseDefinition&lt;/code&gt; must directly or indirectly specialize the base &lt;code&gt;CaseDefinition&lt;/code&gt; &lt;em&gt;&lt;code&gt;Cases::Case&gt;&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Cases-CaseDefinition-checkCaseDefinitionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Cases-CaseDefinition-checkCaseDefinitionSpecialization-_specification">
                     <body>specializesFromLibrary('Cases::Case')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
