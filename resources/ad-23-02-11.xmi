                     <annotatedElement xmi:idref="Systems-Allocations-AllocationDefinition-allocation"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Allocations-AllocationDefinition-allocation-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Allocations-AllocationDefinition-allocation-_upperValue" value="*"/>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Allocations-AllocationUsage" name="AllocationUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Allocations-AllocationUsage-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;AllocationUsage&lt;/code&gt; is a usage of an &lt;code&gt;AllocationDefinition&lt;/code&gt; asserting the allocation of the &lt;code&gt;source&lt;/code&gt; feature to the &lt;code&gt;target&lt;/code&gt; feature.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Allocations-AllocationUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Allocations-AllocationUsage-checkAllocationUsageSpecialization" name="checkAllocationUsageSpecialization">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Allocations-AllocationUsage-checkAllocationUsageSpecialization-_ownedComment.0" body="&lt;p&gt;An &lt;code&gt;AllocationUsage&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;AllocationUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Allocations::allocations&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Allocations-AllocationUsage-checkAllocationUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Allocations-AllocationUsage-checkAllocationUsageSpecialization-_specification">
                     <body>specializesFromLibrary("Allocations::allocations")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Allocations-AllocationUsage-_generalization.0" general="Systems-Connections-ConnectionUsage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Allocations-AllocationUsage-allocationDefinition" name="allocationDefinition" visibility="public" isOrdered="true" isDerived="true" type="Systems-Allocations-AllocationDefinition" association="Systems-Allocations-_packagedElement.0">
                  <redefinedProperty xmi:idref="Systems-Connections-ConnectionUsage-connectionDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Allocations-AllocationUsage-allocationDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;AllocationDefinitions&lt;/code&gt; that are the types of this &lt;code&gt;AllocationUsage&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Allocations-AllocationUsage-allocationDefinition"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Allocations-AllocationUsage-allocationDefinition-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Allocations-AllocationUsage-allocationDefinition-_upperValue" value="*"/>
