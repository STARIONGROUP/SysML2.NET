<?xml version="1.0" encoding="UTF-8"?>
<xmi:XMI xmlns:uml="http://www.omg.org/spec/UML/20161101" xmlns:xmi="http://www.omg.org/spec/XMI/20161101" xmlns:mof="http://www.omg.org/spec/MOF/20161101">
   <uml:Package xmi:type="uml:Package" xmi:id="_sysml" name="SysML" URI="https://www.omg.org/spec/SysML/20230201">
      <packagedElement xmi:type="uml:Package" xmi:id="Systems" name="Systems">
         <packageImport xmi:type="uml:PackageImport" xmi:id="Systems-_packageImport.0">
            <importedPackage href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel"/>
         </packageImport>
         <packagedElement xmi:type="uml:Package" xmi:id="Systems-Ports" name="Ports">
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Ports-PortUsage" name="PortUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;PortUsage&lt;/code&gt; is a usage of a &lt;code&gt;PortDefinition&lt;/code&gt;. A &lt;code&gt;PortUsage&lt;code&gt; itself as well as all its &lt;code&gt;nestedUsages&lt;/code&gt; must be referential (non-composite).&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Ports-PortUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortUsage-validatePortUsageNestedUsagesNotComposite" name="validatePortUsageNestedUsagesNotComposite">
                  <constrainedElement xmi:idref="Systems-Ports-PortUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-validatePortUsageNestedUsagesNotComposite-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;nestedUsages&lt;/code&gt; of a &lt;code&gt;PortUsage&lt;/code&gt; that are not themselves &lt;code&gt;PortUsages&lt;/code&gt; must not be composite.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortUsage-validatePortUsageNestedUsagesNotComposite"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortUsage-validatePortUsageNestedUsagesNotComposite-_specification">
                     <body>nestedUsage-&gt;
    reject(oclIsKindOf(PortUsage))-&gt;
    forAll(not isComposite)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortUsage-checkPortUsageSpecialization" name="checkPortUsageSpecialization">
                  <constrainedElement xmi:idref="Systems-Ports-PortUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-checkPortUsageSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;PortUsage&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;PortUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Ports::ports&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortUsage-checkPortUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortUsage-checkPortUsageSpecialization-_specification">
                     <body>specializesFromLibrary('Ports::ports')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortUsage-checkPortUsageSubportSpecialization" name="checkPortUsageSubportSpecialization">
                  <constrainedElement xmi:idref="Systems-Ports-PortUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-checkPortUsageSubportSpecialization-_ownedComment.0" body="&lt;p&gt;A composite &lt;code&gt;PortUsage&lt;/code&gt; with an &lt;code&gt;owningType&lt;/code&gt; that is a &lt;code&gt;PortDefinition&lt;/code&gt; or &lt;code&gt;PortUsage&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;PortUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Ports::Port::subports&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortUsage-checkPortUsageSubportSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortUsage-checkPortUsageSubportSpecialization-_specification">
                     <body>isComposite and owningType &lt;&gt; null and
(owningType.oclIsKindOf(PortDefinition) or
 owningType.oclIsKindOf(PortUsage)) implies
    specializesFromLibrary('Ports::Port::subports')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortUsage-validatePortUsageIsReference" name="validatePortUsageIsReference">
                  <constrainedElement xmi:idref="Systems-Ports-PortUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-validatePortUsageIsReference-_ownedComment.0" body="&lt;p&gt;Unless a &lt;code&gt;PortUsage&lt;/code&gt; has an &lt;code&gt;owningType&lt;/code&gt; that is a &lt;code&gt;PortDefinition&lt;/code&gt; or a &lt;code&gt;PortUsage&lt;/code&gt;, it must be referential (non-composite).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortUsage-validatePortUsageIsReference"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortUsage-validatePortUsageIsReference-_specification">
                     <body>owningType = null or
not owningType.oclIsKindOf(PortDefinition) and
not owningType.oclIsKindOf(PortUsage) implies
    isReference</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-PortUsage-_generalization.0" general="Systems-Occurrences-OccurrenceUsage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-PortUsage-portDefinition" name="portDefinition" visibility="public" isOrdered="true" isDerived="true" type="Systems-Ports-PortDefinition" association="Systems-Ports-_packagedElement.1">
                  <redefinedProperty xmi:idref="Systems-Occurrences-OccurrenceUsage-occurrenceDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortUsage-portDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;occurrenceDefinitions&lt;/code&gt; of this &lt;code&gt;PortUsage&lt;/code&gt;, which must all be &lt;code&gt;PortDefinitions&lt;code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortUsage-portDefinition"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-PortUsage-portDefinition-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Ports-PortUsage-portDefinition-_upperValue" value="*"/>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Ports-_packagedElement.1" visibility="private">
               <memberEnd xmi:idref="Systems-Ports-_packagedElement.1-definedPort"/>
               <memberEnd xmi:idref="Systems-Ports-PortUsage-portDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Ports-_packagedElement.1-definedPort" name="definedPort" visibility="public" isDerived="true" type="Systems-Ports-PortUsage" association="Systems-Ports-_packagedElement.1">
                  <subsettedProperty xmi:idref="Systems-Occurrences-_packagedElement.7-definedOccurrence"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-_packagedElement.1-definedPort-_ownedComment.0" body="&lt;p&gt;The PortUsages that are typed by a certain PortDefinition.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-_packagedElement.1-definedPort"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-_packagedElement.1-definedPort-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Ports-_packagedElement.1-definedPort-_upperValue" value="*"/>
               </ownedEnd>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Ports-PortDefinition" name="PortDefinition">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;PortDefinition&lt;/code&gt; defines a point at which external entities can connect to and interact with a system or part of a system. Any &lt;code&gt;ownedUsages&lt;/code&gt; of a &lt;code&gt;PortDefinition&lt;/code&gt;, other than &lt;code&gt;PortUsages&lt;/code&gt;, must not be composite.&lt;/p&gt;&#xA;&#xA;&#xA;">
                  <annotatedElement xmi:idref="Systems-Ports-PortDefinition"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortDefinition-derivePortDefinitionConjugatedPortDefinition" name="derivePortDefinitionConjugatedPortDefinition">
                  <constrainedElement xmi:idref="Systems-Ports-PortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-derivePortDefinitionConjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;conjugatedPortDefinition&lt;/code&gt; of a &lt;code&gt;PortDefinition&lt;/code&gt; is the &lt;code&gt;ownedMember&lt;/code&gt; that is a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortDefinition-derivePortDefinitionConjugatedPortDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortDefinition-derivePortDefinitionConjugatedPortDefinition-_specification">
                     <body>conjugatedPortDefinition = 
let conjugatedPortDefinitions : OrderedSet(ConjugatedPortDefinition) =
    ownedMember-&gt;selectByKind(ConjugatedPortDefinition) in
if conjugatedPortDefinitions-&gt;isEmpty() then null
else conjugatedPortDefinitions-&gt;first()
endif</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionOwnedUsagesNotComposite" name="validatePortDefinitionOwnedUsagesNotComposite">
                  <constrainedElement xmi:idref="Systems-Ports-PortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionOwnedUsagesNotComposite-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;ownedUsages&lt;/code&gt; of a &lt;code&gt;PortDefinition&lt;/code&gt; that are not &lt;code&gt;PortUsages&lt;/code&gt; must not be composite.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortDefinition-validatePortDefinitionOwnedUsagesNotComposite"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionOwnedUsagesNotComposite-_specification">
                     <body>ownedUsage-&gt;
    reject(oclIsKindOf(PortUsage))-&gt;
    forAll(not isComposite)</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionConjugatedPortDefinition" name="validatePortDefinitionConjugatedPortDefinition">
                  <constrainedElement xmi:idref="Systems-Ports-PortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionConjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;Unless it is a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;, a &lt;code&gt;PortDefinition&lt;/code&gt; must have exactly one &lt;code&gt;ownedMember&lt;/code&gt; that is a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortDefinition-validatePortDefinitionConjugatedPortDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortDefinition-validatePortDefinitionConjugatedPortDefinition-_specification">
                     <body>not oclIsKindOf(ConjugatedPortDefinition) implies
    ownedMember-&gt;
        selectByKind(ConjugatedPortDefinition)-&gt;
        size() = 1</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-PortDefinition-checkPortDefinitionSpecialization" name="checkPortDefinitionSpecialization">
                  <constrainedElement xmi:idref="Systems-Ports-PortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-checkPortDefinitionSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;PortDefinition&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;PortDefinition&lt;/code&gt; &lt;em&gt;&lt;code&gt;Ports::Port&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortDefinition-checkPortDefinitionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-PortDefinition-checkPortDefinitionSpecialization-_specification">
                     <body>specializeFromLibrary('Ports::Port')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-PortDefinition-_generalization.0" general="Systems-Occurrences-OccurrenceDefinition"/>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-PortDefinition-_generalization.1">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Structures-Structure"/>
               </generalization>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-PortDefinition-conjugatedPortDefinition" name="conjugatedPortDefinition" visibility="public" isDerived="true" type="Systems-Ports-ConjugatedPortDefinition" association="Systems-Ports-_packagedElement.3">
                  <subsettedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Namespaces-Namespace-ownedMember"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortDefinition-conjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;codeConjugatedPortDefinition&lt;/code&gt; that is conjugate to this &lt;code&gt;PortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortDefinition-conjugatedPortDefinition"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-PortDefinition-conjugatedPortDefinition-_lowerValue"/>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Ports-_packagedElement.3" visibility="private">
               <memberEnd xmi:idref="Systems-Ports-PortDefinition-conjugatedPortDefinition"/>
               <memberEnd xmi:idref="Systems-Ports-ConjugatedPortDefinition-originalPortDefinition"/>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Ports-_packagedElement.4" visibility="private">
               <memberEnd xmi:idref="Systems-Ports-_packagedElement.4-portConjugation"/>
               <memberEnd xmi:idref="Systems-Ports-PortConjugation-originalPortDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Ports-_packagedElement.4-portConjugation" name="portConjugation" visibility="public" type="Systems-Ports-PortConjugation" association="Systems-Ports-_packagedElement.4">
                  <subsettedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-_packagedElement.18-conjugation"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-_packagedElement.4-portConjugation-_ownedComment.0" body="&lt;p&gt;The PortConjugation that relates a certain PortDefinition to its ConjugatedPortDefinition (if any).&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-_packagedElement.4-portConjugation"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-_packagedElement.4-portConjugation-_lowerValue"/>
               </ownedEnd>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Ports-ConjugatedPortDefinition" name="ConjugatedPortDefinition">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; is a &lt;code&gt;PortDefinition&lt;/code&gt; that is a &lt;code&gt;PortDefinition&lt;/code&gt; of its original &lt;code&gt;PortDefinition&lt;/code&gt;. That is, a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; inherits all the &lt;code&gt;features&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt;, but input &lt;code&gt;flows&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt; become outputs on the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; and output &lt;code&gt;flows&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt; become inputs on the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;. Every &lt;code&gt;PortDefinition&lt;/code&gt; (that is not itself a &lt;code&gt;&lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;&lt;/code&gt;) has exactly one corresponding &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;, whose effective name is the name of the &lt;code&gt;originalPortDefinition&lt;/code&gt;, with the character &lt;code&gt;~&lt;/code&gt; prepended.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionOriginalPortDefinition" name="validateConjugatedPortDefinitionOriginalPortDefinition">
                  <constrainedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionOriginalPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;originalPortDefinition&lt;/code&gt; of the &lt;code&gt;ownedPortConjugator&lt;/code&gt; of a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; must be the &lt;code&gt;originalPortDefinition&lt;/code&gt; of the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionOriginalPortDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionOriginalPortDefinition-_specification">
                     <body>ownedPortConjugator.originalPortDefinition = originalPortDefinition</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionConjugatedPortDefinitionIsEmpty" name="validateConjugatedPortDefinitionConjugatedPortDefinitionIsEmpty">
                  <constrainedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionConjugatedPortDefinitionIsEmpty-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; must not itself have a &lt;code&gt;conjugatedPortDefinition&lt;/code&gt;.">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionConjugatedPortDefinitionIsEmpty"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-ConjugatedPortDefinition-validateConjugatedPortDefinitionConjugatedPortDefinitionIsEmpty-_specification">
                     <body>conjugatedPortDefinition = null</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-ConjugatedPortDefinition-_generalization.0" general="Systems-Ports-PortDefinition"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-ConjugatedPortDefinition-originalPortDefinition" name="originalPortDefinition" visibility="public" isDerived="true" type="Systems-Ports-PortDefinition" association="Systems-Ports-_packagedElement.3">
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Elements-Element-owningNamespace"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-originalPortDefinition-_ownedComment.0" body="&lt;p&gt;The original &lt;code&gt;PortDefinition&lt;/code&gt; for this &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;, which is the &lt;code&gt;owningNamespace&lt;/code&gt; of the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition-originalPortDefinition"/>
                  </ownedComment>
               </ownedAttribute>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-ConjugatedPortDefinition-ownedPortConjugator" name="ownedPortConjugator" visibility="public" isDerived="true" type="Systems-Ports-PortConjugation" association="Systems-Ports-_packagedElement.7">
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-Type-ownedConjugator"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-ownedPortConjugator-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;PortConjugation&lt;/code&gt; that is the &lt;code&gt;ownedConjugator&lt;/code&gt; of this &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;, linking it to its &lt;code&gt;originalPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition-ownedPortConjugator"/>
                  </ownedComment>
               </ownedAttribute>
               <ownedOperation xmi:type="uml:Operation" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName" name="effectiveName" visibility="public" bodyCondition="Systems-Ports-ConjugatedPortDefinition-effectiveName-unnamed1">
                  <redefinedOperation href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Root-Elements-Element-effectiveName"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName-_ownedComment.0" body="&lt;p&gt;If the &lt;code&gt;name&lt;/code&gt; of the &lt;code&gt;originalPortDefinition&lt;/code&gt; is non-empty, then return that with the character &lt;code&gt;~&lt;/code&gt; prepended.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortDefinition-effectiveName"/>
                  </ownedComment>
                  <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName-unnamed1" name="unnamed1">
                     <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName-unnamed1-_specification">
                        <body>let originalName : String = originalPortDefinition.name in
if originalName = null then null
else '~' + originalName
endif</body>
                        <language>OCL2.0</language>
                     </specification>
                  </ownedRule>
                  <ownedParameter xmi:type="uml:Parameter" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName-_ownedParameter.0" visibility="public" direction="return" effect="create">
                     <type href="http://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#String"/>
                     <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-ConjugatedPortDefinition-effectiveName-_ownedParameter.0-_lowerValue"/>
                  </ownedParameter>
               </ownedOperation>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Ports-PortConjugation" name="PortConjugation">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortConjugation-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;PortConjugation&lt;/code&gt; is a &lt;code&gt;Conjugation&lt;/code&gt; &lt;code&gt;Relationship&lt;/code&gt; between a &lt;code&gt;PortDefinition&lt;/code&gt; and its corresponding &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;. As a result of this &lt;code&gt;Relationship&lt;/code&gt;, the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; inherits all the &lt;code&gt;features&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt;, but input &lt;code&gt;flows&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt; become outputs on the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; and output &lt;code&gt;flows&lt;/code&gt; of the original &lt;code&gt;PortDefinition&lt;/code&gt; become inputs on the &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/code&gt;&lt;/p&gt;&#xA;">
                  <annotatedElement xmi:idref="Systems-Ports-PortConjugation"/>
               </ownedComment>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-PortConjugation-_generalization.0">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-Conjugation"/>
               </generalization>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-PortConjugation-originalPortDefinition" name="originalPortDefinition" visibility="public" type="Systems-Ports-PortDefinition" association="Systems-Ports-_packagedElement.4">
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-Conjugation-originalType"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortConjugation-originalPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;PortDefinition&lt;/code&gt; being conjugated.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortConjugation-originalPortDefinition"/>
                  </ownedComment>
               </ownedAttribute>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-PortConjugation-conjugatedPortDefinition" name="conjugatedPortDefinition" visibility="public" isDerived="true" type="Systems-Ports-ConjugatedPortDefinition" association="Systems-Ports-_packagedElement.7">
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Types-Conjugation-owningType"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-PortConjugation-conjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt; that is conjugate to the &lt;code&gt;originalPortDefinition&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-PortConjugation-conjugatedPortDefinition"/>
                  </ownedComment>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Ports-_packagedElement.7" visibility="private">
               <memberEnd xmi:idref="Systems-Ports-PortConjugation-conjugatedPortDefinition"/>
               <memberEnd xmi:idref="Systems-Ports-ConjugatedPortDefinition-ownedPortConjugator"/>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Ports-ConjugatedPortTyping" name="ConjugatedPortTyping">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortTyping-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;ConjugatedPortTyping&lt;/code&gt; is a &lt;code&gt;FeatureTyping&lt;/code&gt; whose &lt;code&gt;type&lt;/code&gt; is a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;. (This relationship is intended to be an abstract-syntax marker for a special surface notation for conjugated typing of ports.)&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortTyping"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Ports-ConjugatedPortTyping-deriveConjugatedPortTypingPortDefinition" name="deriveConjugatedPortTypingPortDefinition">
                  <constrainedElement xmi:idref="Systems-Ports-ConjugatedPortTyping"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortTyping-deriveConjugatedPortTypingPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;portDefinition&lt;/code&gt; of a &lt;code&gt;ConjugatedPortTyping&lt;/code&gt; is the &lt;code&gt;originalPortDefinition&lt;/code&gt; of the &lt;code&gt;conjugatedPortDefinition&lt;/code&gt; of the &lt;code&gt;ConjugatedPortTyping&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortTyping-deriveConjugatedPortTypingPortDefinition"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Ports-ConjugatedPortTyping-deriveConjugatedPortTypingPortDefinition-_specification">
                     <body>portDefinition = conjugatedPortDefinition.originalPortDefinition</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Ports-ConjugatedPortTyping-_generalization.0">
                  <general href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Features-FeatureTyping"/>
               </generalization>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-ConjugatedPortTyping-portDefinition" name="portDefinition" visibility="public" isDerived="true" type="Systems-Ports-PortDefinition" association="Systems-Ports-_packagedElement.10">
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortTyping-portDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;originalPortDefinition&lt;/code&gt; of the &lt;code&gt;conjugatedPortDefinition&lt;/code&gt; of this &lt;code&gt;ConjugatedPortTyping&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortTyping-portDefinition"/>
                  </ownedComment>
               </ownedAttribute>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Ports-ConjugatedPortTyping-conjugatedPortDefinition" name="conjugatedPortDefinition" visibility="public" type="Systems-Ports-ConjugatedPortDefinition" association="Systems-Ports-_packagedElement.9">
                  <redefinedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Features-FeatureTyping-type"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-ConjugatedPortTyping-conjugatedPortDefinition-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;type&lt;/code&gt; of this &lt;code&gt;ConjugatedPortTyping&lt;/code&gt; considered as a &lt;code&gt;FeatureTyping&lt;/code&gt;, which must be a &lt;code&gt;ConjugatedPortDefinition&lt;/code&gt;.&lt;/p&gt;&#xA;">
                     <annotatedElement xmi:idref="Systems-Ports-ConjugatedPortTyping-conjugatedPortDefinition"/>
                  </ownedComment>
               </ownedAttribute>
            </packagedElement>
            <packagedElement xmi:type="uml:Association" xmi:id="Systems-Ports-_packagedElement.9" visibility="private">
               <memberEnd xmi:idref="Systems-Ports-_packagedElement.9-typingByConjugatedPort"/>
               <memberEnd xmi:idref="Systems-Ports-ConjugatedPortTyping-conjugatedPortDefinition"/>
               <ownedEnd xmi:type="uml:Property" xmi:id="Systems-Ports-_packagedElement.9-typingByConjugatedPort" name="typingByConjugatedPort" visibility="public" type="Systems-Ports-ConjugatedPortTyping" association="Systems-Ports-_packagedElement.9">
                  <subsettedProperty href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Core-Features-_packagedElement.1-typingByType"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Ports-_packagedElement.9-typingByConjugatedPort-_ownedComment.0" body="&lt;p&gt;The ConjugatedPortTypings whose &lt;code&gt;conjugatedPortDefinition&lt;/code&gt; a certain ConjugatedPortDefinition.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Ports-_packagedElement.9-typingByConjugatedPort"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Ports-_packagedElement.9-typingByConjugatedPort-_lowerValue"/>
                  <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="Systems-Ports-_packagedElement.9-typingByConjugatedPort-_upperValue" value="*"/>
