            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Actions-DecisionNode" name="DecisionNode">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-DecisionNode-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;DecisionNode&lt;/code&gt; is a &lt;code&gt;ControlNode&lt;/code&gt; that makes a selection from its outgoing &lt;code&gt;Successions&lt;/code&gt;.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Actions-DecisionNode"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeIncomingSuccessions" name="validateDecisionNodeIncomingSuccessions">
                  <constrainedElement xmi:idref="Systems-Actions-DecisionNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeIncomingSuccessions-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;DecisionNode&lt;code&gt; may have at most one incoming &lt;code&gt;Succession&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-DecisionNode-validateDecisionNodeIncomingSuccessions"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeIncomingSuccessions-_specification">
                     <body>targetConnector-&gt;selectByKind(Succession)-&gt;size() &lt;= 1</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeOutgoingSuccessions" name="validateDecisionNodeOutgoingSuccessions">
                  <constrainedElement xmi:idref="Systems-Actions-DecisionNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeOutgoingSuccessions-_ownedComment.0" body="&lt;p&gt;All outgoing &lt;code&gt;Successions&lt;/code&gt; from a &lt;code&gt;DecisionNode&lt;/code&gt; must have a target &lt;code&gt;multiplicity&lt;/code&gt; of &lt;code&gt;0..1&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-DecisionNode-validateDecisionNodeOutgoingSuccessions"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-DecisionNode-validateDecisionNodeOutgoingSuccessions-_specification">
                     <body>sourceConnector-&gt;selectAsKind(Succession)-&gt;
    collect(connectorEnd-&gt;at(2))-&gt;
    forAll(targetMult |
        multiplicityHasBounds(targetMult, 0, 1))</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeSpecialization" name="checkDecisionNodeSpecialization">
                  <constrainedElement xmi:idref="Systems-Actions-DecisionNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;DecisionNode&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;ActionUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Actions::Action::decisions&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-DecisionNode-checkDecisionNodeSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeSpecialization-_specification">
                     <body>specializesFromLibrary("Actions::Action::decisions")</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeOutgoingSuccessionSpecialization" name="checkDecisionNodeOutgoingSuccessionSpecialization">
                  <constrainedElement xmi:idref="Systems-Actions-DecisionNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeOutgoingSuccessionSpecialization-_ownedComment.0" body="&lt;p&gt;All outgoing &lt;code&gt;Successions&lt;code&gt; from a &lt;code&gt;DecisionNode&lt;/code&gt; must subset the inherited &lt;em&gt;&lt;code&gt;outgoingHBLink&lt;/code&gt;&lt;/em&gt; &lt;code&gt;feature&lt;/code&gt; of the &lt;code&gt;DecisionNode&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-DecisionNode-checkDecisionNodeOutgoingSuccessionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-DecisionNode-checkDecisionNodeOutgoingSuccessionSpecialization-_specification">
                     <body>sourceConnector-&gt;selectByKind(Succession)-&gt;
    forAll(subsetsChain(this, 
        resolveGlobal("ControlPerformances::MergePerformance::outgoingHBLink")))</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Actions-DecisionNode-_generalization.0" general="Systems-Actions-ControlNode"/>
            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Actions-MergeNode" name="MergeNode">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-MergeNode-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;MergeNode&lt;/code&gt; is a &lt;code&gt;ControlNode&lt;/code&gt; that asserts the merging of its incoming &lt;code&gt;Successions&lt;/code&gt;. A &lt;code&gt;MergeNode&lt;/code&gt; may have at most one outgoing &lt;code&gt;Successions&lt;/code&gt;.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Actions-MergeNode"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions" name="validateMergeNodeOutgoingSuccessions">
                  <constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;MergeNode&lt;/code&gt; may have at most one outgoing &lt;code&gt;Succession&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions-_specification">
                     <body>sourceConnector-&gt;selectAsKind(Succession)-&gt;size() &lt;= 1</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions" name="validateMergeNodeIncomingSuccessions">
                  <constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions-_ownedComment.0" body="&lt;p&gt;All incoming &lt;code&gt;Successions&lt;/code&gt; to a &lt;code&gt;MergeNode&lt;/code&gt; must have a source &lt;code&gt;multiplicity&lt;/code&gt; of &lt;code&gt;0..1&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions-_specification">
                     <body>targetConnector-&gt;selectByKind(Succession)-&gt;
    collect(connectorEnd-&gt;at(1))-&gt;
    forAll(sourceMult |
        multiplicityHasBounds(sourceMult, 0, 1))</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization" name="checkMergeNodeIncomingSuccessionSpecialization">
                  <constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization-_ownedComment.0" body="&lt;p&gt;All incoming &lt;code&gt;Successions&lt;code&gt; to a &lt;code&gt;MergeNode&lt;/code&gt; must subset the inherited &lt;em&gt;&lt;code&gt;incomingHBLink&lt;/code&gt;&lt;/em&gt; &lt;code&gt;feature&lt;/code&gt; of the &lt;code&gt;MergeNode&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization-_specification">
                     <body>targetConnector-&gt;selectByKind(Succession)-&gt;
    forAll(subsetsChain(this, 
        resolveGlobal("ControlPerformances::MergePerformance::incomingHBLink")))</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-MergeNode-checkMergeNodeSpecialization" name="checkMergeNodeSpecialization">
