            </packagedElement>
            <packagedElement xmi:type="uml:Class" xmi:id="Systems-Actions-WhileLoopActionUsage" name="WhileLoopActionUsage">
               <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;WhileLoopActionUsage&lt;/code&gt; is a &lt;code&gt;LoopActionUsage&lt;/code&gt; that specifies that the &lt;code&gt;bodyClause&lt;/code&gt; &lt;code&gt;ActionUsage&lt;/code&gt; should be performed repeatedly while the result of the &lt;code&gt;whileArgument&lt;/code&gt; &lt;code&gt;Expression&lt;/code&gt; is true or until the result of the &lt;code&gt;untilArgument&lt;/code&gt; &lt;code&gt;Expression&lt;/code&gt; (if provided) is true. The &lt;code&gt;whileArgument&lt;/code&gt; &lt;code&gt;Expression&lt;/code&gt; is evaluated before each (possible) performance of the &lt;code&gt;bodyClause&lt;/code&gt;, and the &lt;code&gt;untilArgument&lt;/code&gt; &lt;code&gt;Expression&lt;/code&gt; is evaluated after each performance of the &lt;code&gt;bodyClause&lt;/code&gt;.&lt;/p&gt;">
                  <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage"/>
               </ownedComment>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSubactionSpecialization" name="checkWhileLoopActionUsageSubactionSpecialization">
                  <constrainedElement xmi:idref="Systems-Actions-WhileLoopActionUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSubactionSpecialization-_ownedComment.0" body="&lt;p&gt;A composite &lt;code&gt;WhileLoopActionUsage&lt;/code&gt; that is a subaction usage must directly or indirectly specialize the &lt;code&gt;ActionUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Actions::Action::whileLoops&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSubactionSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSubactionSpecialization-_specification">
                     <body>isSubactionUsage() implies
    specializesFromLibrary('Actions::Action::whileLoops')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageUntilArgument" name="deriveWhileLoopActionUsageUntilArgument">
                  <constrainedElement xmi:idref="Systems-Actions-WhileLoopActionUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageUntilArgument-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;whileArgument&lt;/code&gt; of a &lt;code&gt;WhileLoopActionUsage&lt;/code&gt; is its third input &lt;code&gt;parameter&lt;/code&gt;, which, if it exists, must be an &lt;code&gt;Expression&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageUntilArgument"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageUntilArgument-_specification">
                     <body>untilArgument =
    let parameter : Feature = inputParameter(3) in
    if parameter &lt;&gt; null and parameter.oclIsKindOf(Expression) then
        parameter.oclAsType(Expression)
    else
        null
    endif
</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSpecialization" name="checkWhileLoopActionUsageSpecialization">
                  <constrainedElement xmi:idref="Systems-Actions-WhileLoopActionUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSpecialization-_ownedComment.0" body="&lt;p&gt;A &lt;code&gt;WhileLoopActionUsage&lt;/code&gt; must directly or indirectly specialize the &lt;code&gt;ActionUsage&lt;/code&gt; &lt;em&gt;&lt;code&gt;Actions::whileLoopActions&lt;/code&gt;&lt;/em&gt; from the Systems Model Library.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSpecialization"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-WhileLoopActionUsage-checkWhileLoopActionUsageSpecialization-_specification">
                     <body>specializesFromLibrary('Actions::whileLoopActions')</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <ownedRule xmi:type="uml:Constraint" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageWhileArgument" name="deriveWhileLoopActionUsageWhileArgument">
                  <constrainedElement xmi:idref="Systems-Actions-WhileLoopActionUsage"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageWhileArgument-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;whileArgument&lt;/code&gt; of a &lt;code&gt;WhileLoopActionUsage&lt;/code&gt; is its first input &lt;code&gt;parameter&lt;/code&gt;, which must be an &lt;code&gt;Expression&lt;/code&gt;.&lt;/p&gt;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageWhileArgument"/>
                  </ownedComment>
                  <specification xmi:type="uml:OpaqueExpression" xmi:id="Systems-Actions-WhileLoopActionUsage-deriveWhileLoopActionUsageWhileArgument-_specification">
                     <body>whileArgument =
    let parameter : Feature = inputParameter(1) in
    if parameter &lt;&gt; null and parameter.oclIsKindOf(Expression) then
        parameter.oclAsType(Expression)
    else
        null
    endif
</body>
                     <language>OCL2.0</language>
                  </specification>
               </ownedRule>
               <generalization xmi:type="uml:Generalization" xmi:id="Systems-Actions-WhileLoopActionUsage-_generalization.0" general="Systems-Actions-LoopActionUsage"/>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Actions-WhileLoopActionUsage-whileArgument" name="whileArgument" visibility="public" isDerived="true" association="Systems-Actions-_packagedElement.21">
                  <type href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Functions-Expression"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-whileArgument-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;Expression&lt;/code&gt; whose result, if true, determines that the &lt;code&gt;bodyAction&lt;/code&gt; should continue to be performed. It is the first owned &lt;code&gt;parameter&lt;/code&gt; of the &lt;code&gt;WhileLoopActionUsage&lt;/code&gt;.&lt;/p&gt; &#xA;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-whileArgument"/>
                  </ownedComment>
               </ownedAttribute>
               <ownedAttribute xmi:type="uml:Property" xmi:id="Systems-Actions-WhileLoopActionUsage-untilArgument" name="untilArgument" visibility="public" isDerived="true" association="Systems-Actions-_packagedElement.20">
                  <type href="https://www.omg.org/spec/KerML/20230201/KerML.xmi#Kernel-Functions-Expression"/>
                  <ownedComment xmi:type="uml:Comment" xmi:id="Systems-Actions-WhileLoopActionUsage-untilArgument-_ownedComment.0" body="&lt;p&gt;The &lt;code&gt;Expression&lt;/code&gt; whose result, if false, determines that the &lt;code&gt;bodyAction&lt;/code&gt; should continue to be performed. It is the (optional) third owned &lt;code&gt;parameter&lt;/code&gt; of the &lt;code&gt;WhileLoopActionUsage&lt;/code&gt;.&lt;/p&gt; &#xA;">
                     <annotatedElement xmi:idref="Systems-Actions-WhileLoopActionUsage-untilArgument"/>
                  </ownedComment>
                  <lowerValue xmi:type="uml:LiteralInteger" xmi:id="Systems-Actions-WhileLoopActionUsage-untilArgument-_lowerValue"/>
               </ownedAttribute>
